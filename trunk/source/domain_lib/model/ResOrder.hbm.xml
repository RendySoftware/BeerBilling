<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
xmlns="urn:nhibernate-mapping-2.2"
namespace="domain_lib.model"
assembly="domain_lib">
  <class name="ResOrder" table="RESORDER" lazy="false">
    <id name="Id" column="ID" unsaved-value="-1">
      <generator class="identity" />
    </id>

    <property name="BillId">
      <column name="BILL_ID" not-null="false" />
    </property>

    <property name="MenuId">
      <column name="MENU_ID" not-null="false" />
    </property>

    <property name="Amount">
      <column name="AMOUNT" not-null="false" />
    </property>

    <property name="CreatedBy">
      <column name="CREATED_BY" not-null="false" />
    </property>

    <property name="CreatedDate">
      <column name="CREATED_DATE" not-null="false" />
    </property>

    <property name="UpdatedBy">
      <column name="UPDATED_BY" not-null="false" />
    </property>

    <property name="UpdatedDate">
      <column name="UPDATED_DATE" not-null="false" />
    </property>

  </class>
</hibernate-mapping>